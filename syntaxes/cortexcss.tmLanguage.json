{
  "fileTypes": [],
  "injectionSelector": "L:source.ts",
  "patterns": [
    {
      "begin": "@",
      "end": ":",
      "name": "entity.name.variable.css.ts"
    },
    {
      "match": ":header-bg",
      "name": "entity.name.value.css.ts"
    },
    {
      "match": "(\\.[\\w-]+(?:\\:[\\w-]+)*(?=\\s*:\\s*))|(:host.*(?=\\s*:\\s*))|(::slotted.*(?=\\s*:\\s*))",
      "name": "entity.name.class.css.ts"
    },
    {
      "match": "(export default)",
      "name": "keyword.control.module.ts"
    },
    {
      "match": "([a-z]+)(?=: `)",
      "name": "entity.name.function.ts"
    },
    {
      "match": "\\$\\w+(?:-\\w+){0,2}",
      "name": "variable.other.css.ts"
    },
    {
      "match": "\\S+",
      "name": "text.unmatched.css.ts"
    }
  ],
  "scopeName": "source.css.ts"
}
